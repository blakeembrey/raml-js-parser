{
  "name": "raml-parser",
  "version": "0.8.11",
  "description": "A RAML parser based on PyYAML written in CoffeScript and available for use as NodeJs module or in-browser.",
  "repository": {
    "type": "git",
    "url": "git@github.com:raml-org/raml-js-parser.git"
  },
  "main": "./lib/raml.js",
  "bin": {
    "raml-parser": "bin/raml-parser"
  },
  "browser": {
    "got": false
  },
  "dependencies": {
    "got": "~6.7.1",
    "json-lint": "~0.1.0",
    "pluralize": "~4.0.0",
    "q": "1.5.0",
    "uritemplate": "0.3.4"
  },
  "devDependencies": {
    "browserify": "~14.3.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "coffeeify": "2.1.0",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-coffeelint": "0.0.16",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-connect": "1.0.2",
    "grunt-contrib-uglify": "2.3.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-mocha-cli": "3.0.0",
    "grunt-mocha-phantomjs": "4.0.0",
    "grunt-shell": "~2.1.0",
    "load-grunt-tasks": "~3.5.2",
    "mocha": "3.3.0",
    "pre-commit": "~1.2.2"
  },
  "scripts": {
    "test": "grunt test",
    "build": "grunt build",
    "stage-build": "git add lib && git add dist"
  },
  "pre-commit": [
    "test",
    "build",
    "stage-build"
  ]
}
